{
    "name": "loopback-authorization-extension",
    "version": "0.1.0",
    "description": "HRBAC, MAC, DAC access models generic implementation for Loopback v4",
    "keywords": [
        "loopback-extension",
        "loopback"
    ],
    "main": "index.js",
    "engines": {
        "node": ">=8.9"
    },
    "scripts": {
        "build:apidocs": "lb-apidocs",
        "build": "lb-tsc es2017 --outDir dist",
        "build:watch": "lb-tsc --watch --outDir dist",
        "clean": "lb-clean dist",
        "pretest": "npm run clean && npm run build",
        "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
        "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js",
        "prepublishOnly": "npm run test"
    },
    "repository": {
        "type": "git"
    },
    "author": "",
    "license": "",
    "files": [
        "README.md",
        "index.js",
        "index.d.ts",
        "dist",
        "src",
        "!*/__tests__"
    ],
    "dependencies": {
        "@loopback/boot": "^1.4.0",
        "@loopback/context": "^1.19.0",
        "@loopback/core": "^1.8.1",
        "@loopback/rest": "^1.15.0"
    },
    "devDependencies": {
        "@loopback/build": "^1.7.1",
        "@loopback/testlab": "^1.5.0",
        "@types/node": "^10.11.2",
        "typescript": "~3.5.1"
    }
}
