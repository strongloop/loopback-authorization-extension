{
    "name": "loopback-authorization-extension",
    "version": "5.1.0",
    "description": "HRBAC, MAC, DAC access models, generic implementation for Loopback v4",
    "keywords": [
        "loopback-extension",
        "loopback"
    ],
    "main": "index.js",
    "engines": {
        "node": ">=8.9"
    },
    "scripts": {
        "start": "nodemon index.ts --exec ts-node",
        "prebuild": "lb-tsc --outDir build/temp",
        "build": "parcel build --out-dir build --cache-dir build/temp/cache --target node build/temp/index.js",
        "postbuild": "lb-clean build/temp",
        "test": "lb-mocha --allow-console-logs \"test/**/*.test.ts\"",
        "clean": "lb-clean build",
        "prepublishOnly": "npm run test"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/strongloop/loopback-authorization-extension"
    },
    "author": "KoLiBer",
    "license": "MIT",
    "files": [
        "README.md",
        "index.js",
        "index.ts",
        "build",
        "src",
        "test"
    ],
    "dependencies": {
        "@loopback/boot": "^1.5.10",
        "@loopback/context": "^1.23.4",
        "@loopback/core": "^1.10.6",
        "@loopback/repository": "^1.15.3",
        "@loopback/rest": "^1.22.0",
        "@loopback/service-proxy": "^1.3.12",
        "loopback-history-extension": "latest"
    },
    "devDependencies": {
        "@loopback/build": "^2.0.15",
        "@loopback/testlab": "^1.9.3",
        "@types/node": "^10.17.0",
        "nodemon": "^2.0.1",
        "parcel-bundler": "^1.12.4",
        "source-map-support": "^0.5.16",
        "typescript": "~3.5.3"
    }
}
