{
    "name": "loopback-authorization-extension",
    "version": "6.0.0",
    "description": "HRBAC, MAC, DAC access models, generic implementation for Loopback v4",
    "keywords": [
        "loopback-extension",
        "loopback"
    ],
    "main": "index.js",
    "engines": {
        "node": ">=8.9"
    },
    "scripts": {
        "clean": "lb-clean dist build *.tsbuildinfo",
        "prebuild": "npm run clean",
        "build": "lb-tsc --outDir dist",
        "postbuild": "parcel build --out-dir build --cache-dir build/cache --target node dist/index.js",
        "build:watch": "lb-tsc --watch --outDir dist",
        "start": "node .",
        "start:watch": "nodemon .",
        "test": "lb-mocha --allow-console-logs \"test/**/*.test.ts\"",
        "prepublishOnly": "npm run build"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/strongloop/loopback-authorization-extension"
    },
    "author": "KoLiBer",
    "license": "MIT",
    "files": [
        "README.md",
        "index.js",
        "index.d.ts",
        "dist"
    ],
    "dependencies": {
        "@loopback/boot": "^1.5.12",
        "@loopback/context": "^1.24.0",
        "@loopback/core": "^1.11.0",
        "@loopback/repository": "^1.15.5",
        "@loopback/rest": "^1.24.0",
        "loopback-history-extension": "latest"
    },
    "devDependencies": {
        "@loopback/build": "^2.1.0",
        "@loopback/testlab": "^1.9.5",
        "@types/node": "^10.17.6",
        "module-alias": "^2.2.2",
        "nodemon": "^2.0.1",
        "parcel-bundler": "^1.12.4",
        "source-map-support": "^0.5.16",
        "typescript": "~3.5.3"
    },
    "_moduleAliases": {
        "~": "./dist"
    }
}
