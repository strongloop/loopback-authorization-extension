{
    "name": "loopback-authorization-extension",
    "version": "3.0.0",
    "description": "HRBAC, MAC, DAC access models, generic implementation for Loopback v4",
    "keywords": [
        "loopback-extension",
        "loopback"
    ],
    "main": "index.js",
    "engines": {
        "node": ">=8.9"
    },
    "scripts": {
        "build": "lb-tsc --outDir build",
        "build:watch": "lb-tsc --watch --outDir build",
        "clean": "lb-clean build *.tsbuildinfo",
        "pretest": "npm run clean && npm run build",
        "test": "lb-mocha --allow-console-logs \"build/__tests__\"",
        "test:dev": "lb-mocha --allow-console-logs build/__tests__/**/*.js",
        "prepublishOnly": "npm run test"
    },
    "repository": {
        "type": "git"
    },
    "author": "KoLiBer",
    "license": "MIT",
    "files": [
        "README.md",
        "index.js",
        "index.d.ts",
        "build",
        "src",
        "!*/__tests__"
    ],
    "dependencies": {
        "@loopback/boot": "^1.5.10",
        "@loopback/context": "^1.23.4",
        "@loopback/core": "^1.10.6",
        "@loopback/repository": "^1.15.3",
        "@loopback/rest": "^1.22.0",
        "loopback-connector-mysql": "^5.4.2",
        "loopback-history-extension": "latest"
    },
    "devDependencies": {
        "@loopback/build": "^2.0.15",
        "@loopback/testlab": "^1.9.3",
        "@types/node": "^10.17.0",
        "source-map-support": "^0.5.16",
        "typescript": "~3.5.3"
    }
}
